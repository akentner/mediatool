    "incrontab": "/etc/incron.d/mediatool",
    "lockfile": "$APP_PATH$/var/incron.lock",
    "bin": "$APP_PATH$/bin/mediatool",
    "paths": [
      "/data/_incoming/btsync"
    ],
    "ignore": [
      ".SyncID",
      ".!sync"
    ]
  }
